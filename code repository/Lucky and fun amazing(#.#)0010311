print("I'm bording...", "What can I do?", "Ya, i going to do somethings.", "(:^.^:)")
Py = input().strip()
Minecraft = 0
import random
e = Py * random.randrange(7, 7474747)
while True:
    M = input().strip
    Minecraft = type(s) for +: M(builtin_function_or_method) and Py(str(Py))
    if Minecraft >= e:
        print("Yay~! Wo-ho!!! I did it!!!", "(:☆.☆:)")
        break
    elif Minecraft <= 0:
        print("??? what??? I did it?!? Wo ho~!", "(:l.l:)")
        break
    else:
        print("Oh my,", "I'll try agaen!!", "(:>.<:)")
        Minecraft = 0