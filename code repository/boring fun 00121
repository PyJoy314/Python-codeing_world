Py = int(input()) or 0
import random
e = Py * random.randrange(7, 7474747)
while True:
    M = int(input())
    R = M += Py
    print(R)
    if R >= e:
        print("Yay~! Wo-ho!!! I did it!!!")
        break
        elif R <= 0:
            print("??? what??? I did it?!? Wo ho~!")
            break
            else:
                print("Oh my,", "I'll try agaen!!")
                print(R)
                M += 1